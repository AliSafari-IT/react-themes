#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/d/repos/asafarim/node_modules/.pnpm/tsup@8.5.0_@swc+core@1.12.9_f733ee61df2e3831d453fb3d01fc1f28/node_modules/tsup/dist/node_modules:/proc/cygdrive/d/repos/asafarim/node_modules/.pnpm/tsup@8.5.0_@swc+core@1.12.9_f733ee61df2e3831d453fb3d01fc1f28/node_modules/tsup/node_modules:/proc/cygdrive/d/repos/asafarim/node_modules/.pnpm/tsup@8.5.0_@swc+core@1.12.9_f733ee61df2e3831d453fb3d01fc1f28/node_modules:/proc/cygdrive/d/repos/asafarim/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/d/repos/asafarim/node_modules/.pnpm/tsup@8.5.0_@swc+core@1.12.9_f733ee61df2e3831d453fb3d01fc1f28/node_modules/tsup/dist/node_modules:/proc/cygdrive/d/repos/asafarim/node_modules/.pnpm/tsup@8.5.0_@swc+core@1.12.9_f733ee61df2e3831d453fb3d01fc1f28/node_modules/tsup/node_modules:/proc/cygdrive/d/repos/asafarim/node_modules/.pnpm/tsup@8.5.0_@swc+core@1.12.9_f733ee61df2e3831d453fb3d01fc1f28/node_modules:/proc/cygdrive/d/repos/asafarim/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-node.js" "$@"
fi
